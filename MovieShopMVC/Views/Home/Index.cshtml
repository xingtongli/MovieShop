@model IEnumerable<ApplicationCore.Models.MovieCardResponseModel>

@{
    ViewData["Title"] = "Home Page";
}

@*Razor Views in ASP.NET Combine Razor Syntax with HTML
Combine C# and HTML here*@

<div class="container-fluid">
    <div class="row">
        @foreach(var movie in Model)
        {
            <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                <partial model="movie" name="_MovieCard" />
            </div>

        }
    </div>
</div>
